package kr.ac.kopo.day04;

public class ArrayMain02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 1,2,3,4,5를 가지는 배열 arr를 생성
		
		//arr배열의 원소 출력
		
		int[] arr = new int[5];
		
		for(int i=0; i < arr.length; i++) {
			
			arr[i] = i+1;
			System.out.println();
		}
		
	}

}
