package kr.ac.kopo.day02;

public class IFMain01 {

	public static void main(String[] args) {
		
		int a = 10, b = 5 ;
		
		System.out.println("MAX : " + ( a > b ?a:b)); // »ïÇ×¿¬»êÀÚ
		
		if (a>b) {
			System.out.println("Max : " + a);
		} else {
			System.out.println("Max : " + b);
			
		}

	}

}
