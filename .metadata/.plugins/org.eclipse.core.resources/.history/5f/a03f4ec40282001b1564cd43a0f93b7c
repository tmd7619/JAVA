/*	5)
*	*****
*	-****
*	--***
*   ---**
*   ----*
*   ---**
*   --***
*   -****  
*	*****
*/

public class ExamMain5_4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i =1 ; i <= 9 ; i++) {
			for(int j = 1; j <= 5; j++) {
				if() {
					System.out.print("-");
				} else{ 
					System.out.print("*");
				}
			}
			
			
			System.out.println();
		}
		
	}

}
