package kr.ac.kopo.day03.exem;



/*
 *  1)
 *  12345
 * 	23456
 * 	34567
 * 	45678
 *  56789
 *  
 *  2)
 *  56789
 *  45678
 *  34567
 *  23456
 *  12345
 *  
 *  3)
 *  *****
 *  ****
 *  ***
 *  **
 *  *
 *  
 *  
 *  4)
 *  *
 *  **
 *  ***
 *  ****
 *  
 *  
 */	

public class ExamMain01 {

	public static void main(String[] args) {
		
		//1
		for(int i = 1; i <= 5; i++) {
			for(int j = i ;j <= i+4 ; j++) {
				System.out.print(j);
			}
			
			System.out.println();
		}
		
		System.out.println("--------------------------");
		
		//2
		for(int i = 5; i <= 9; i++) {
			for(int j = i ;j <= i +4 ; j++) {
				System.out.print(j);
			}
			
			System.out.println();
		}
		
		
	}
}
