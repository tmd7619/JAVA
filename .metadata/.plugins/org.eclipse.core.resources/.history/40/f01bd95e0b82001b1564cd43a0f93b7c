package kr.ac.kopo.day04;

public class Loopmain01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for(int i = 1; i <= 3; i++ ) {
			for(int j = 1 ; j <= 5; j++) {
				if(j==3) {
					break;
				}
				System.out.print(j);
			}
			System.out.println();
		}
			

	}

}
