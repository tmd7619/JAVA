package kr.ac.kopo.day03.exem;


/*	5)
*	*****
*	-****
*	--***
*   ---**
*   ----*
*   ---**
*   --***
*   -****  
*	*****
*/

public class ExamMain5_3 {

	public static void main(String[] args) {
		
		int space = 0;
		for(int i =1 ; i <= 9 ; i++) {
			for(int j =1 ; j <= space ; j++) {
				System.out.print("-");
			}
			System.out.print("*");
			System.out.println();
		}
	}
}
