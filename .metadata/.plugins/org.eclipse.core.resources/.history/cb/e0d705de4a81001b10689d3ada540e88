package kr.ac.kopo.day03;

/*
 * *****
 * *****
 * *****
 * 
 */


public class LoopMain02 {

	public static void main(String[] args) {
		
		int cnt = 1;
		while(cnt <= 3) {
			System.out.println("*****");
			++cnt;
			
		System.out.println("-----------------------------");
		for(int i=1; i<=3 ; ++i){
			System.out.println("*****");
		}
			
		for(int i02 = 1; i02 <= 3; ++i02) {
			System.out.print("*");
			System.out.print("*");
			System.out.print("*");
			System.out.print("*");
			System.out.print("*");
			System.out.println();
			
		}
		
		
		}
	}
}
